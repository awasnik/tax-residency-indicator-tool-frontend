{% extends "includes/layout.njk" %}

{% from "macros/title.njk"                         import title %}

{% block pageTitle %}
  {{ title(messages("inconclusiveResult.title")) }}
{% endblock %}

{% block mainContent %}

  <div class="govuk-main-wrapper">
    <div class="govuk-grid-row">
      <div class="govuk-grid-column-two-thirds">

        <h1 class="govuk-heading-xl">
          {{ messages("inconclusiveResult.heading") }}
        </h1>
        <p class="govuk-body-l">{{ messages("inconclusiveResult.p1") }}</p>
        <p class="govuk-body">{{ messages("inconclusiveResult.p2") }}</p>

        <p class="govuk-body">{{ messages("common.result.further.info") }}</p>
        <ul class="govuk-list govuk-list--bullet">
            <li>{{ messages("common.result.further.info.l1") }} <a href="https://www.gov.uk/government/publications/rdr3-statutory-residence-test-srt/guidance-note-for-statutory-residence-test-srt-rdr3" target="_blank">{{ messages("common.statutoryResidenceTest") }}</a> </li>
            <li>{{ messages("common.result.further.info.l2") }} <a href="https://www.gov.uk/contact-hmrc" target="_blank">{{ messages("common.result.further.info.hmrc") }}</a></li>
            <li>{{ messages("common.result.further.info.l3") }} <a href="https://www.gov.uk/tax-help" target="_blank">{{ messages("common.result.further.info.accountant") }}</a></li>
        </ul>
      </div>
    </div>
  </div>

{% endblock %}
